<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:util="clr-namespace:ExampleMAUI.Util"
             xmlns:viewModels="clr-namespace:ExampleMAUI.ViewModels"
             x:Class="ExampleMAUI.Views.InteractionVisualizationView"
             x:DataType="viewModels:InteractionVisualizationViewModel">
  <util:InteractionGraphicsView
    Background="{StaticResource GrayLight}"
    InteractionList="{Binding Interactions}">
    <util:InteractionGraphicsView.Drawable>
      <util:InteractionsDrawable/>
    </util:InteractionGraphicsView.Drawable>
  </util:InteractionGraphicsView>
</ContentView>
