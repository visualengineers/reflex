<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <OutputType>WinExe</OutputType>
    <PackageId>ReFlex.Utilities.MouseInputConverter</PackageId>
    <Authors />
    <Copyright>Copyright ©  2020</Copyright>
    <Company>GTV - Gesellschaft fuer Technische Visualistik mbH, Dresden</Company>
    <Version>0.1.0</Version>
    <Product>ReFlex.Utilities</Product>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <AssemblyName>ReFlex.Utilities.MouseInputConverter</AssemblyName>
    <RootNamespace>ReFlex.Utilities.MouseInputConverter</RootNamespace>
    <Platforms>x64</Platforms>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
